ull);\n  const [currentUser, setCurrentUser] = useState(null);\n  const [currentDay, setCurrentDay] = useState('');\n  const [meals, setMeals] = useState([]);\n  const history = useHistory();\n    \n    useEffect(() => {\n     \n      const fetchMeals = async () => {\n        const mealData = await getAllMeals();\n        setMeals(mealData)\n      }\n      fetchMeals();\n    }, [])\n\n  useEffect(() => {\n    const handleVerify = async () => {\n      const userData = await verifyUser();\n      setCurrentUser(userData);\n      if (!userData) {\n        history.push('/')\n      }\n    }\n    handleVerify();\n  }, [])\n\n  //=================\n  //===== auth ======\n  //=================\n\n  const handleLogin = async (loginData) => {\n    const userData = await loginUser(loginData);\n    setCurrentUser(userData);\n    history.push('/home');\n  }\n\n  const handleRegister = async (registerData) => {\n    const userData = await registerUser(registerData);\n    setCurrentUser(userData);\n    history.push('/home');\n  }\n\n  const handleLogout = () => {\n    setCurrentUser(null);\n    localStorage.removeItem('authToken');\n    removeToken();\n    history.push('/');\n  }\n\n  //=================\n  //===== meals =====\n  //=================\n\n  const handleDelete = async (id) => {\n    await destroyMeal(id);\n    setMeals(prevState => prevState.filter(meal => meal.id !== id))\n  }\n\n  const handleCreate = async (mealData) => {\n    const newMeal = await postMeal({...mealData, day_id: currentDay.id})\n    setMeals(prevState => [...prevState, newMeal])\n  }\n\n  const handleUpdate = async (id, mealData) => {\n    const updatedMeal = await putMeal(id, mealData);\n    setMeals(prevState => prevState.map(meal => {\n      return meal.id === Number(id) ? updatedMeal : meal\n    }))\n  }\n\n  return (\n      <div className=\"App\">\n        <Switch>\n\n        <Route exact path='/'>\n          <Login handleLogin={handleLogin} handleRegister={handleRegister}/>\n        </Route>\n\n        <Route exact path='/account'>\n          <Account currentUser={currentUser} handleLogout={handleLogout}/>\n        </Route>\n\n        <Route exact path='/home'>\n          <Home \n          meals={meals}\n          handleDelete={handleDelete}\n          setCurrentDay={setCurrentDay}\n          currentUser={currentUser}\n          handleCreate={handleCreate}\n          handleUpdate={handleUpdate}\n          setEditingId={setEditingId}\n          />\n        </Route>\n\n        <Route exact path='/stats'>\n          <Stats />\n        </Route>\n        </Switch>\n      </div>\n  );\n}\n\nexport default App;\n","/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Header.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/index.js",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Footer.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Login.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Account.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Navbar.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/HomeFooter.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Home.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Stats.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/BreakfastTable.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/DeleteIcon.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/EditIcon.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/componen