reate}\n          handleUpdate={handleUpdate}\n          setEditingId={setEditingId}\n          />\n        </Route>\n\n        <Route exact path='/stats'>\n          <Stats />\n        </Route>\n        </Switch>\n      </div>\n  );\n}\n\nexport default App;\n","/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Header.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/index.js",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Footer.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Login.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Account.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/Navbar.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/shared/HomeFooter.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Home.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/screens/Stats.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/BreakfastTable.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/DeleteIcon.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/EditIcon.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/AddIcon.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/LunchTable.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/SnacksTable.jsx",[],"/home/brian/General_Assembly/SEI/Pheonix/unit_4/P4/WhenIEat/client/when-i-eat/src/components/DinnerTable.jsx",[],"/ho